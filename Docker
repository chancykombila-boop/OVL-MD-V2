} catch (e) { await conn.sendMessage(m.chat, { text: 'âš ï¸ Erreur avec RemoveBG. Ajoute ta clÃ© API pour activer la fonction.' }, { quoted: m }); } }; removebgHandler.command = ['removebg'];

