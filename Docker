// === REMOVE BACKGROUND === export const removebgHandler = async (m, { conn }) => { if (!m.quoted || !m.quoted.mimetype || !m.quoted.mimetype.startsWith('image/')) { return conn.sendMessage(m.chat, { text: 'ğŸ–¼ï¸ RÃ©ponds Ã  une image pour supprimer le fond.' }, { quoted: m }); }

